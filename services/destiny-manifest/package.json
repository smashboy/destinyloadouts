{
  "name": "@services/destiny-manifest",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "dotenv -e .env.local -- tsx watch src",
    "db:docker": "docker compose up -d",
    "db:migrate-dev": "dotenv -e .env.local -- npx prisma migrate dev",
    "db:manifest": "dotenv -e .env.local -- tsx ./prisma/fetchDestinyManifestTables.ts",
    "db:reset": "dotenv -e .env.local -- npx prisma migrate reset --force --preview-feature",
    "prisma:generate": "npx prisma generate",
    "prisma:studio": "dotenv -e .env.local -- npx prisma studio"
  },
  "dependencies": {
    "@destiny/shared": "*",
    "@fastify/cors": "^8.2.1",
    "@prisma/client": "^4.11.0",
    "@trpc/server": "^10.17.0",
    "bungie-api-ts": "^4.22.2",
    "fastify": "^4.15.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/node": "18.15.0",
    "dotenv": "^16.0.3",
    "prisma": "^4.11.0",
    "tailwindcss": "^3.2.7",
    "tsx": "^3.12.5",
    "typescript": "4.9.5"
  }
}
